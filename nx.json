{
  "npmScope": "sectester",
  "affected": {
    "defaultBase": "master"
  },
  "workspaceLayout": {
    "appsDir": "examples",
    "libsDir": "packages"
  },
  "generators": {
    "@nrwl/node:library": {
      "buildable": true,
      "publishable": true,
      "pascalCaseFiles": true,
      "testEnvironment": "node",
      "setParserOptionsProject": true
    }
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "nx.json": "*"
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "publish": {
      "dependsOn": ["build"]
    },
    "build": {
      "dependsOn": ["^build"],
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "test": {
      "cache": true
    },
    "e2e": {
      "cache": true
    }
  },
  "parallel": 1
}

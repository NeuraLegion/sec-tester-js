{
  "name": "sectester-js",
  "version": "0.31.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NeuraLegion/sectester-js.git"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=10"
  },
  "workspaces": [
